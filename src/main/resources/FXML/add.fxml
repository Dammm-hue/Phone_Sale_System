<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.Rectangle?>
<?import javafx.scene.text.Font?>

<StackPane maxHeight="350.0" maxWidth="300.0" minHeight="350.0" minWidth="300.0" style="-fx-background-color: transparent;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.add_controller">
    <children>
        <VBox alignment="BOTTOM_CENTER" maxHeight="350.0" maxWidth="300.0" minHeight="350.0" minWidth="300.0" style="-fx-background-color: gray;">
            <StackPane.margin>
                <Insets />
            </StackPane.margin>
            <children>
                <VBox minWidth="300.0" spacing="3.0" style="-fx-background-color: white; -fx-background-radius: 15;">
                    <children>
                        <VBox alignment="CENTER" prefHeight="116.0" prefWidth="305.0">
                            <children>
                                <HBox minHeight="49.0" minWidth="305.0" spacing="8.0">
                           <children>
                              <StackPane alignment="CENTER_LEFT" HBox.hgrow="ALWAYS">
                                 <children>
                                    <Label fx:id="lblid" text="ID">
                                       <font>
                                          <Font name="System Bold" size="14.0" />
                                       </font>
                                    </Label>
                                    <Button fx:id="btnsave" mnemonicParsing="false" onAction="#clicksave">
                                       <StackPane.margin>
                                          <Insets left="250.0" />
                                       </StackPane.margin></Button>
                                    <HBox fx:id="buttonbox" alignment="CENTER_RIGHT" spacing="5.0" visible="false">
                                       <children>
                                          <Button fx:id="btnedit" mnemonicParsing="false" onAction="#clickedit" />
                                          <Button fx:id="btndetail" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#clickdetail" />
                                       </children>
                                       <padding>
                                          <Insets left="225.0" />
                                       </padding>
                                    </HBox>
                                 </children>
                                 <padding>
                                    <Insets right="17.0" />
                                 </padding>
                              </StackPane>
                           </children>
                           <padding>
                              <Insets left="8.0" right="8.0" />
                           </padding>
                                </HBox>
                                <TextField fx:id="txtuser" alignment="CENTER" prefHeight="35.0" prefWidth="305.0" promptText="User Name" style="-fx-background-color: transparent;">
                                    <VBox.margin>
                                        <Insets />
                                    </VBox.margin>
                           <font>
                              <Font size="19.0" />
                           </font>
                                </TextField>
                        <PasswordField fx:id="pwdpassword" alignment="CENTER" promptText="Password" style="-fx-background-color: transparent;">
                           <font>
                              <Font size="14.0" />
                           </font>
                        </PasswordField>
                            </children>
                     <padding>
                        <Insets top="5.0" />
                     </padding>
                        </VBox>
                        <VBox alignment="CENTER" minHeight="132.0" minWidth="300.0" spacing="9.0">
                            <children>
                        <HBox alignment="CENTER_LEFT" layoutX="30.0" layoutY="10.0">
                           <children>
                              <HBox alignment="CENTER_LEFT" spacing="10.0" HBox.hgrow="ALWAYS">
                                 <children>
                                    <Button fx:id="btnlocation" mnemonicParsing="false">
                                       <HBox.margin>
                                          <Insets />
                                       </HBox.margin>
                                    </Button>
                                    <Label text="Location">
                                       <HBox.margin>
                                          <Insets />
                                       </HBox.margin>
                                       <font>
                                          <Font name="System Bold" size="16.0" />
                                       </font>
                                    </Label>
                                 </children>
                                 <padding>
                                    <Insets left="5.0" />
                                 </padding>
                              </HBox>
                              <TextField fx:id="txtaddress" alignment="CENTER_RIGHT" promptText="Address" style="-fx-padding: 5; -fx-background-color: transparent;" HBox.hgrow="ALWAYS">
                                 <HBox.margin>
                                    <Insets />
                                 </HBox.margin>
                                 <font>
                                    <Font name="System Italic" size="15.0" />
                                 </font>
                              </TextField>
                           </children>
                        </HBox>
                        <HBox alignment="CENTER_LEFT" layoutX="30.0" layoutY="61.0">
                           <children>
                              <HBox spacing="10.0" HBox.hgrow="ALWAYS">
                                 <padding>
                                    <Insets left="5.0" />
                                 </padding>
                                 <children>
                                    <Button fx:id="btnphone" mnemonicParsing="false">
                                       <HBox.margin>
                                          <Insets />
                                       </HBox.margin>
                                    </Button>
                                    <Label text="Phone">
                                       <HBox.margin>
                                          <Insets />
                                       </HBox.margin>
                                       <font>
                                          <Font name="System Bold" size="16.0" />
                                       </font>
                                    </Label>
                                 </children>
                              </HBox>
                              <TextField fx:id="txtphone" alignment="CENTER_RIGHT" promptText="Phone Number" style="-fx-background-color: transparent;" HBox.hgrow="ALWAYS">
                                 <HBox.margin>
                                    <Insets />
                                 </HBox.margin>
                                 <font>
                                    <Font name="System Italic" size="15.0" />
                                 </font>
                              </TextField>
                           </children>
                        </HBox>
                        <HBox alignment="CENTER_LEFT" layoutX="15.0" layoutY="116.0">
                           <children>
                              <HBox spacing="10.0" HBox.hgrow="ALWAYS">
                                 <children>
                                    <Button fx:id="btnemail" mnemonicParsing="false">
                                       <HBox.margin>
                                          <Insets />
                                       </HBox.margin>
                                    </Button>
                                    <Label text="Email">
                                       <HBox.margin>
                                          <Insets />
                                       </HBox.margin>
                                       <font>
                                          <Font name="System Bold" size="16.0" />
                                       </font>
                                    </Label>
                                 </children>
                                 <padding>
                                    <Insets left="5.0" />
                                 </padding>
                              </HBox>
                              <TextField fx:id="txtemail" alignment="CENTER_RIGHT" promptText="Email" style="-fx-background-color: transparent;" HBox.hgrow="ALWAYS">
                                 <HBox.margin>
                                    <Insets />
                                 </HBox.margin>
                                 <font>
                                    <Font name="System Italic" size="15.0" />
                                 </font>
                              </TextField>
                           </children>
                        </HBox>
                            </children>
                        </VBox>
                    </children>
            </VBox>
            </children>
        </VBox>
        <Rectangle fx:id="rectimage" arcHeight="40.0" arcWidth="40.0" fill="#f4f6f8" height="150.0" onMouseClicked="#uploadimage" stroke="BLACK" strokeType="INSIDE" translateY="-100.0" width="156.0" />
    </children>
</StackPane>
